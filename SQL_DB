CREATE DATABASE MOVIE       //creating a database

USE MOVIE;                  //using database

CREATE TABLE MOVIEDETAILS(
MOVIENAME VARCHAR(20) PRIMARY KEY,
ACTOR VARCHAR(20),                                          //creating a table
ACTRESS VARCHAR(20),
YOR VARCHAR(20),
DIRECTOR VARCHAR(20));

